<h3><%= @board.title %></h3>
<nav>
  <span>Add Pin: <%= render partial: 'pins/form' %> </span>
</nav>
<% if @board.pins %>
<ul>
  <% @board.pins.each do |pin| %>
  <li>
    <%= pin.title %>
    <%= pin.user.email %>
    <%= link_to "delete pin", board_pin_path(@board,pin), method: 'delete' %>
  </li>
  <% end %>
</ul>
<% end %>
